  "icon": "images/nonsensify-logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajxv/nonsensify.git"
  },
  "bugs": {
    "url": "https://github.com/ajxv/nonsensify/issues"
  },
  "homepage": "https://github.com/ajxv/nonsensify#readme",
